{
  "name": "eupg",
  "displayName": "EUPG",
  "description": "EUPG language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "eupg",
      "aliases": ["EUPG", "eupg"],
      "extensions": [".eupg",".EUPG"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "eupg",
      "scopeName": "main.eupg",
      "path": "./syntaxes/eupg.tmLanguage.json"
    }]
  }
}
