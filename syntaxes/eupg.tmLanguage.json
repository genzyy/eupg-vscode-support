{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "EUPG",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.eupg",
				"match": "\\b(if|and|or|let)\\b"
			}]
		},
		"strategies": {
			"patterns": [{
				"name": "keyword.control.eupg",
				"match": "\\b(strategy [A-Z]+)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.eupg",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.eupg",
					"match": "\\\\."
				}
			]
		},
		"entities": {
			"patterns": [{
				"name": "entity.name.class.eupg",
				"match": "\\b(bot|myself|enemy|healthpack|elixir|allies|antimatter|board|allybase)\\b"
			}]
		},
		"actions": {
			"patterns": [{
				"name": "entity.name.method.eupg",
				"match": "\\b(moveAway)\\b"
			}]
		},
		"metrics": {
			"patterns": [{
				"name": "entity.name.method.eupg",
				"match": "\\b(Health|Closest|Farthest...)\\b"
			}]
		},
		"properties": {
			"patterns": [{
				"name": "entity.name.method.eupg",
				"match": "\\b(SpawnTime|Position)\\b"
			}]
		}
	},
	"scopeName": "better.eupg"
}